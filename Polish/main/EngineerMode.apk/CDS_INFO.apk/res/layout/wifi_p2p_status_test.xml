            <RadioButton android:id="@id/wifi_p2p_operation_channel_6" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="@string/wifi_p2p_operation_channel_6" />
            <RadioButton android:id="@id/wifi_p2p_operation_channel_11" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="@string/wifi_p2p_operation_channel_11" />
            <RadioButton android:id="@id/wifi_p2p_operation_channel_36" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="@string/wifi_p2p_operation_channel_36" />
