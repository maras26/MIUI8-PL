                <requestFocus />
            </EditText>
        </LinearLayout>
        <LinearLayout style="@style/entry_layout">
            <Button android:textSize="14.0sp" android:id="@id/wifi_p2p_ST_do" android:layout_width="fill_parent" android:layout_height="wrap_content" android:layout_marginTop="8.0dip" android:text="@string/wifi_p2p_ST_do" />
