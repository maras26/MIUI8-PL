<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="FoldersAppBaseTheme" parent="@android:style/Theme.Light" />
    <style name="FoldersAppTheme" parent="@style/FoldersAppBaseTheme" />
    <style name="FoldersDialog" parent="@android:style/Theme.Dialog">
        <item name="android:windowBackground">@drawable/folder_dialog_background</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsFloating">true</item>
    </style>
    <style name="FoldersActivityTheme" parent="@android:style/Theme.Wallpaper">
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowFrame">@null</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsTranslucent">false</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:backgroundDimEnabled">false</item>
    </style>
    <style name="FoldersTheme" parent="@android:style/Theme">
        <item name="android:colorForeground">#ffffffff</item>
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowFrame">@null</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:backgroundDimEnabled">true</item>
    </style>
    <style name="FoldersThemeNoTitleBar">
        <item name="android:windowNoTitle">true</item>
    </style>
    <style name="FolderSmallRatingBar" parent="@android:style/Widget.RatingBar">
        <item name="android:maxHeight">24.0dip</item>
        <item name="android:progressDrawable">@drawable/folder_rating_small_bar</item>
        <item name="android:minHeight">24.0dip</item>
    </style>
    <style name="FolderNegativeDialogButton">
        <item name="android:textSize">16.0sp</item>
        <item name="android:textColor">#ff000000</item>
        <item name="android:gravity">center</item>
        <item name="android:layout_width">0.0dip</item>
        <item name="android:layout_height">36.0dip</item>
        <item name="android:layout_marginLeft">20.0dip</item>
        <item name="android:layout_marginRight">12.0dip</item>
        <item name="android:singleLine">true</item>
        <item name="android:layout_weight">1.0</item>
    </style>
    <style name="FolderPositiveDialogButton">
        <item name="android:textSize">16.0sp</item>
        <item name="android:textColor">#ffffffff</item>
        <item name="android:gravity">center</item>
        <item name="android:layout_width">0.0dip</item>
        <item name="android:layout_height">36.0dip</item>
        <item name="android:layout_marginLeft">12.0dip</item>
        <item name="android:layout_marginRight">20.0dip</item>
        <item name="android:singleLine">true</item>
        <item name="android:layout_weight">1.0</item>
    </style>
    <style name="FolderTransparent" parent="@android:style/Theme">
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:backgroundDimEnabled">true</item>
    </style>
    <style name="ProgressButton" />
    <style name="ProgressButton.Pin" parent="@style/ProgressButton">
        <item name="android:enabled">false</item>
        <item name="android:focusable">false</item>
        <item name="android:clickable">false</item>
        <item name="progress">0</item>
        <item name="max">100</item>
        <item name="circleColor">@color/progress_default_circle_color</item>
        <item name="progressColor">@color/progress_default_progress_color</item>
        <item name="pinnedDrawable">@null</item>
        <item name="unpinnedDrawable">@null</item>
        <item name="shadowDrawable">@null</item>
        <item name="innerSize">@dimen/progress_inner_size</item>
        <item name="pinned">false</item>
        <item name="animating">false</item>
        <item name="animationSpeed">1</item>
        <item name="animationDelay">50</item>
        <item name="animationStripWidth">6</item>
    </style>
    <style name="ProgressButton.Pin.Compat" parent="@style/ProgressButton.Pin">
        <item name="android:background">@android:drawable/list_selector_background</item>
    </style>
    <style name="ProgressButton.1" parent="@style/ProgressButton" />
    <style name="ProgressButton.2" parent="@style/ProgressButton">
        <item name="pinned">true</item>
    </style>
    <style name="ProgressButton.Clickable" parent="@style/ProgressButton">
        <item name="android:focusable">true</item>
        <item name="android:clickable">true</item>
    </style>
    <style name="ProgressButton.Clickable.3" parent="@style/ProgressButton.Clickable">
        <item name="pinned">true</item>
    </style>
    <style name="ProgressButton.Clickable.4" parent="@style/ProgressButton.Clickable">
        <item name="circleColor">@color/holo_green_dark</item>
        <item name="progressColor">@color/holo_green_light</item>
    </style>
</resources>
